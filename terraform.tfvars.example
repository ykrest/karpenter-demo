# AWS Region
region = "eu-central-1"

# EKS Cluster Configuration
cluster_name    = "karpenter-demo"
cluster_version = "1.34"

# VPC Configuration
vpc_cidr = "10.0.0.0/16"

# NAT Gateway Configuration
# Set to true for production, false for cost savings in dev
enable_nat_gateway = true
single_nat_gateway = false  # Set to true to use only 1 NAT Gateway (cost savings)

# Environment
environment = "dev"

# Karpenter Version
karpenter_version = "1.8.1"
